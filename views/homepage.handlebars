<!-- Loop over each object in the songs array -->
{{#each songs as |song| }}
<div class="row mb-4 project">
  <div class="col-md-5">
    <h2>
      <a href="/song/{{song.id}}">{{song.name}}</a>
    </h2>

    {{!-- <p>Created by {{song.user.name}} on {{format_date project.date_created}}</p> --}}
  </div>
  <div class="col-md-7">
    <p>
      {{song.description}}
    </p>
  </div>
</div>
{{/each}}
