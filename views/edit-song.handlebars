        <H2>Edit your song</H2>
        <label for="song-name">Song: {{song_name}}</label>
      </div>
     
      <div class="form-group">
        <label for="artist">Artist: {{artist}}</label>
   
      </div>
      <div class="form-group">
        <label for="score">Score: </label>
        <textarea class="form-input" id="updated-score-content" name="score">{{score}}</textarea>
      </div>
      <div class="form-group">
        <label for="description">Description: </label>
        <textarea class="form-input" id="updated-description-content" name="description">{{description}}</textarea>
      </div>
