      <div class="form-group">
         <button type="button" id="updateBtn" class="btn btn-primary" data-update-id="{{id}}">Edit Song</button>
      </div>
        <div class="form-group song-list">
        <button type="button" class="btn btn-sm btn-danger" data-id="{{id}}">DELETE</button>
   </div>
